
@{
    ViewBag.Title = "Home";
    Layout = "~/Views/_LayoutPage.cshtml";
}

<div class="container-fluid" style="margin: 140px 0 10px">
    <div id="carouselExampleControls" class="carousel w-25 mx-auto" data-bs-ride="carousel">
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img class="d-block w-100" style="border-radius: 50%" src="~/Content/images/logo1.png">
            </div>
            <div class="carousel-item">
                <img class="d-block w-100" style="border-radius: 50%" src="~/Content/images/logo2.png">
            </div>
        </div>
        <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-bs-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-bs-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
        </a>
    </div>
</div>